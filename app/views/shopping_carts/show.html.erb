<h1>Your Cart</h1>
<hr>
<% @cart_items.each do |cart_item| %>
    <%=  "#{cart_item.quantity}   #{cart_item.item.name} "%><br>
    <%= "$ #{cart_item.item.price}  each" %>
    <%= button_to "delete", {:controller => :selected_items, :action => 'destroy', :id => cart_item.id }, :method => :delete %><br><br>
<% end %>

<%= link_to "Return to All Items", items_path %><br><br>


